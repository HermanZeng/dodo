<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">

    <class name="ReadingProgress" table="reading_progress">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="total" type="java.lang.Integer" column="total"/>
        <property name="current" type="java.lang.Integer" column="current"/>
        <property name="date" type="java.lang.String" column="date"/>
        <property name="userId" type="java.lang.String" column="user_id"/>
        <property name="bookId" type="java.lang.String" column="book_id"/>
    </class>

</hibernate-mapping>

